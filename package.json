{
  "name": "react-base",
  "version": "1.0.1",
  "description": "React Base",
  "main": "index.js",
  "author": "Simon Chelebyan <schelebyan@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "start": "./node_modules/.bin/babel-node server",
    "dev": "WEBPACK_ENV=dev npm start",
    "prod": "WEBPACK_ENV=production npm start",
    "dev_build": "WEBPACK_ENV=dev npm run bundle",
    "prod_build": "WEBPACK_ENV=production npm run bundle",
    "bundle": "./node_modules/.bin/webpack --config webpack.config.js",
    "clean": "rm -rf content/bundle.js content/bundle.js.map npm-debug.log",
    "test": "nyc mocha-webpack --webpack-config webpack.config-test.babel.js --require babel-register --require ./testSetup.js 'src/**/*tests.js'",
    "tdd": "mocha-webpack --webpack-config webpack.config-test.babel.js --require babel-register --require ./testSetup.js 'src/**/*tests.js' --watch",
    "lint": "./node_modules/.bin/eslint ./src;", 
    "spawn_duck": "./scripts/spawn_duck.sh", 
    "spawn_view": "./scripts/spawn_view.sh"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      ".tmp",
      "**/*tests.js",
      "src/api",
      "testSetup.js",
      "src/config",
      "webpack.config-test.babel.js",
      "**/*reducers.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "express": "^4.13.4",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "mocha-webpack": "^0.7.0",
    "nyc": "^11.2.1",
    "react-test-renderer": "^16.0.0",
    "redbox-react": "^1.2.2",
    "sinon": "^4.0.1",
    "webpack": "^2.7.0",
    "webpack-config-loader": "^0.1.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "amazon-cognito-identity-js": "^1.21.0",
    "amazon-cognito-js": "^1.1.0",
    "aws-api-gateway-client": "^0.2.9",
    "aws-sdk": "^2.131.0",
    "axios": "^0.9.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es5-property-mutators": "^6.24.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-2017": "0.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.7",
    "config": "^1.19.0",
    "css-loader": "^0.23.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.12.0",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "node-sass": "^4.5.3",
    "node-uuid": "^1.4.7",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.0",
    "react-cookie": "^0.4.5",
